#integridade_dois-$rede=`cat /tmp/lista_imagem-$rede.txt`
#integridade_tres-$rede=`cat /tmp/lista_audio-$rede.txt`
#integridade_quatro-$rede=`cat /tmp/video_mute-$rede.txt`



